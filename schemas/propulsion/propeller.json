{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://setuav.com/schemas/propulsion/propeller.json",
    "title": "Propeller Schema",
    "type": "object",
    "properties": {
        "tag": {
            "$ref": "../common.json#/definitions/tag"
        },
        "type": {
            "const": "propeller"
        },
        "manufacturer": {
            "type": "string"
        },
        "model": {
            "type": "string"
        },
        "diameter": {
            "type": "number",
            "minimum": 0
        },
        "pitch": {
            "type": "number",
            "minimum": 0
        },
        "blade_count": {
            "type": "integer",
            "minimum": 1
        },
        "direction": {
            "enum": [
                "CW",
                "CCW"
            ]
        },
        "hub_diameter": {
            "type": "number",
            "minimum": 0
        },
        "hub_thickness": {
            "type": "number",
            "minimum": 0
        },
        "mass": {
            "$ref": "../common.json#/definitions/mass"
        },
        "material": {
            "type": "string"
        }
    },
    "required": [
        "tag",
        "diameter",
        "pitch",
        "blade_count",
        "direction",
        "mass"
    ]
}
